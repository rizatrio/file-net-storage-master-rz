<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox style="-fx-font-family: serif" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.example.PrimaryController">
   <children>
      <Label fx:id="label" text="Primary View" />
      <Button text="Switch to Secondary View" onAction="#switchToSecondary"/>
      <Button text="Upload file" onAction="#uploadFile"/>

   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
